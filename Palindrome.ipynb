   ],
   "source": [
    "our_input = \"EY EDİP ADANADA PİDE YE\"\n",
    "word = \"\"\n",
    "for i in our_input.casefold():\n",
    "  if i.isalnum():\n",
    "    word += i\n",
    "if word == word[::-1]:\n",
    "  print(f\"{our_input} is a palindrome\")\n",
    "else:\n",
    "  print(f\"{our_input} is not a palindrome\")"
   ]
  }
