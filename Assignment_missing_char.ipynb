    "def missing_char(word, n):\n",
    "    \n",
    "    list_word = []\n",
    "    for i in word:\n",
    "        list_word.append(i)\n",
    "    \n",
    "    if n < len(list_word):\n",
    "            \n",
    "        return \"\".join(list_word[0:n] + list_word[(n + 1) : ])\n",
    "    else:\n",
    "        return \"Index Out of Range\""
   ]
